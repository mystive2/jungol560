import java.util.Scanner;
 
public class Main {
 
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int []A=new int[10];
        int min=1000;
        for(int i=0;i<A.length;i++)
        {
            A[i]=sc.nextInt();
        }
        for(int i=0;i<A.length;i++)
        {
            if(A[i]<min)
            {
                min=A[i];
            }
        }
        sc.close();
        System.out.println(min);
    }
}
